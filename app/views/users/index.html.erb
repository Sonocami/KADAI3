<%=render'user',user:@user%>
<!--booksにある部分テンプレート-->

<h1>Users</h1>
<%@users.each do |user|%>
<%= image_tag user.get_profile_image(100,100)%>
<%=user.name %>
<%=link_to "show","user/#{user.id}"%>
<%end%>

<% if @book.errors.any?%>
<%=@book.errors.count %> error prohibited this obj from being saved:
<ul><%@book.errors.full_messages.each do |message|%>
<li><%=message%></li>
<%end%>
<%end%>